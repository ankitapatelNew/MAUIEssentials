<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUIEssentialsApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUIEssentialsApp"
    xmlns:pages="clr-namespace:MAUIEssentialsApp.Pages"
    xmlns:appResources="clr-namespace:MAUIEssentials.AppResources;assembly=MAUIEssentials"
    xmlns:controls="clr-namespace:MAUIEssentials.AppCode.Controls;assembly=MAUIEssentials"
    Shell.FlyoutBehavior="Flyout"
    Shell.ForegroundColor="White"
    Shell.FlyoutItemIsVisible="True"
    x:Name="appShellPage">

     <ShellContent
        Route="SplashPage"
        ContentTemplate="{DataTemplate pages:SplashPage}"/>

    <ShellContent
        Route="FingerprintPage"
        ContentTemplate="{DataTemplate pages:FingerprintPage}"/>

    <ShellContent
        Route="LoginPage"
        ContentTemplate="{DataTemplate pages:LoginPage}"/>

    <ShellContent
        Route="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}"/>

</Shell>
